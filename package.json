{
  "name": "n8n-nodes-aftership",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for AfterShip shipment tracking API supporting 1,100+ carriers, tracking management, shipping labels, returns, and AI-powered delivery predictions.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aftership.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-aftership#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aftership/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "aftership",
    "shipment-tracking",
    "tracking",
    "shipping",
    "logistics",
    "ecommerce",
    "delivery",
    "courier",
    "fedex",
    "ups",
    "usps",
    "dhl",
    "returns",
    "labels",
    "webhook"
  ],
  "main": "dist/nodes/AfterShip/AfterShip.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AfterShipApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AfterShip/AfterShip.node.js",
      "dist/nodes/AfterShip/AfterShipTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "mkdir -p dist/nodes/AfterShip && cp nodes/AfterShip/*.svg dist/nodes/AfterShip/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
